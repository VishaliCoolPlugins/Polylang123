{
    "name": "wp-syntex/linguator",
    "description": "Adds multilingual capability to WordPress",
    "type": "wordpress-plugin",
    "license": "GPL-3.0-or-later",
    "authors": [
        {
            "name": "WP SYNTEX",
            "homepage": "https://linguator.pro"
        }
    ],
    "require": {
        "php": ">=7.2"
    },
    "autoload": {
        "classmap": [
            "integrations/",
            "modules/",
            "admin/",
            "frontend/",
            "include/",
            "install/",
            "settings/"
        ],
        "psr-4": {
            "WP_Syntex\\Linguator\\": "include/"
        }
    },
    "config": {
        "optimize-autoloader": true
    }
} 